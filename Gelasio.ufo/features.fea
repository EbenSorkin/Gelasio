# Prefix: Languagesystems
languagesystem latn dflt;
languagesystem latn LTH ;
languagesystem latn ROM ;

feature aalt {
  feature locl;
} aalt;

feature locl {
 # Localized Forms
 script latn; # Latin
 language LTH  exclude_dflt; # Lithuanian
    sub iacute by dottediacute;
 language ROM  exclude_dflt; # Romanian
    sub [Scedilla scedilla Tcedilla tcedilla] by [Scommaaccent scommaaccent Tcommaaccent tcommaaccent];
} locl;

